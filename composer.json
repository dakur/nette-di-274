		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/database": "^3.1",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.1",
		"nette/http": "^3.1",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.3",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"latte/latte": "^2.11 || ^3.0",
		"tracy/tracy": "^2.8",
		"grifart/nette-scoped-di": "^0.1.0",
		"league/flysystem": "^3.12"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"symfony/thanks": "^1"
